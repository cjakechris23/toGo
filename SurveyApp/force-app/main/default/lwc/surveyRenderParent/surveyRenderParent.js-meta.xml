<?xml version="1.0" encoding="UTF-8"?>
<LightningComponentBundle xmlns="http://soap.sforce.com/2006/04/metadata">
    <apiVersion>56.0</apiVersion>
    <isExposed>true</isExposed>
    <masterLabel>Render Survey LWC</masterLabel>
    <description>Custom LWC to render custom survey form</description>
    <targets>
        <target>lightning__AppPage</target>
        <target>lightning__FlowScreen</target>
    </targets>
    <targetConfigs>
        <targetConfig targets="lightning__FlowScreen">
            <property name="customSurveyVersionId" type="String" label='Custom Survey Version Id' role="inputOnly" />
            <property name="customSurveyInvitationId" type="String" label='Custom Survey Invitation Id' role="inputOnly" />
            <property name="participantContactId" type="String" label='Participant Contact Id' role="inputOnly" />
        </targetConfig>
    </targetConfigs>
</LightningComponentBundle>