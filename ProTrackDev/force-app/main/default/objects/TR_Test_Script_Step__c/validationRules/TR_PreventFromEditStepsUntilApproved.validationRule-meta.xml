<ValidationRule xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>TR_PreventFromEditStepsUntilApproved</fullName>
    <active>false</active>
    <errorConditionFormula>ISPICKVAL( TR_Test_Script__r.TR_Approval_Status__c  , &apos;Pending Approval&apos;)</errorConditionFormula>
